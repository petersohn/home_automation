
include_rules


include $(COMPILE_TUP)
include $(GEN_CPP_TUP)

INCLUDE_DIRS += -I$(ESP8266WIFI_SRC_DIR)

: device.cpp.template |> !gen_cpp |> device.cpp
: server.cpp.template |> !gen_cpp |> server.cpp
: foreach *.cpp |> !cxx |>

include $(LINK_TUP)

: *.o $(WIFI_LIB) $(ESP8266WIFI_LIB) | $(CORE_LIB) |> !linker |> test.elf

include $(FLASH_TUP)

: test.elf |> !elf2bin |> test.bin
: test.bin |> !flash |>
