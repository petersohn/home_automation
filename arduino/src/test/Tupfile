
include_rules


include $(COMPILE_TUP)

INCLUDE_DIRS += -I$(ESP8266WIFI_SRC_DIR)

: foreach *.cpp |> !cxx |>

include $(LINK_TUP)

: *.o $(WIFI_LIB) $(ESP8266WIFI_LIB) $(HTTP_LIB) $(_CONFIG_LIB) | $(CORE_LIB) |> !linker |> test.elf

include $(FLASH_TUP)

: test.elf |> !elf2bin |> test.bin
: test.bin |> !flash |>
